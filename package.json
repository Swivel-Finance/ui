{
  "name": "@swivel-finance/ui",
  "description": "UI behaviors and components for Swivel frontends.",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Swivel-Finance/ui.git"
  },
